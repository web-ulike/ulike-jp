      "blocks": {
        "question_qmtTqj": {
          "type": "question",
          "settings": {
            "title": "全身に使用可能ですか。",
            "text": "<p>全身に使用可能です。また、カートリッジの交換も不要です。</p>",
            "highlighted_text": "none",
