<style>
     .section-{{ section.id }}-padding {
      padding-top: calc({{ section.settings.padding_top }}px);
      padding-bottom: calc({{ section.settings.padding_bottom }}px);
    }
    @media screen and (max-width: 768px){
    .section-{{ section.id }}-padding {
      padding-top: calc({{ section.settings.padding_top }}px * 0.6);
      padding-bottom: calc({{ section.settings.padding_bottom }}px * 0.6);
    }
  }
  .module14_wrapper{
    /* padding: 100px 0; */
  }
  .module14_box{
    display: flex;
    gap: 70px;
  }

  .module14_right-cnt{
    background: #FBF9F3;
    padding: 60px;
    margin-top: 250px;
  }
  .module14_cnt-tle1{
    color: #000;
  font-family: var(--font-hd-family);
  font-size: 30px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  }
  .module14_cnt-box{
    margin-top: 35px;
    display: flex;
    gap: 20px;
  }
  .module14_cnt-box .module14_cnt-li{
    /* max-width: 210px; */
    flex:1;
  }
  .module14_cnt-box .module14_li-m1{
    color: #000;
  font-size: 15px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  }
  .module14_cnt-box .module14_li-m2{

    background-color: #F0E9D5;
    margin-top: 15px;
  }
  .module14_cnt-box .module14_li-m3{
    color: #000;
  font-size: 16px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  margin-top: 10px;
  }
  .module14_cnt-box .module14_li-m4{
    color: #000;
    font-size: 12px;
    font-style: normal;
    font-weight: 300;
    line-height: 150%;
    margin-top: 15px;
  }
  .module14_right .module_right-note{
    color: rgba(0, 0, 0, 0.50);
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 150%;
    margin-top: 30px;
  }

  @media (max-width: 767px){
    .module14_wrapper{
      /* padding: 30px 0; */
    }
    .module14_box{
      flex-direction: column-reverse;
      gap: 20px;
    }
    .module14_left picture{
      width: 68%;
    }
    .module14_right-cnt{
      padding: 20px;
      background: #fff;
      margin-top: 0px;
    }
    .module14_cnt-tle1{
      font-size: 18px;
      text-align: center;
    }
    .module14_cnt-box{
      margin-top: 30px;
    }
    .module14_right .module_right-note{
      font-size: 10px;
      padding: 0 20px;
    }
    .module14_cnt-box{
      overflow-x: scroll;
      scrollbar-width: none;
      scrollbar-color: transparent transparent;
    }
    .module14_cnt-box .module14_cnt-li {
      flex-shrink: 0;
      flex-basis: 80%;
      max-width: 100%;
  }
  .module14_cnt-box .module14_li-m1{
    font-size: 12px;
  }
  .module14_cnt-box .module14_li-m3{
    margin-top: 20px;
    font-size: 15px;
  }
  .module14_cnt-box .module14_li-m4{
    margin-top: 10px;
  }
  }
</style>
<section>
  <div class="module14_wrapper section-{{ section.id }}-padding">
    <div class="custom_container">
      <div class="module14_box">

        <div class="module14_right to-top" style="transition-delay: 0.2s;">
          <div class="module14_right-cnt">
            {%- if section.settings.tle_1 != blank -%}
              <div class="module14_cnt-tle1">
                {{ section.settings.tle_1 }}
              </div>
            {%- endif -%}
            <div class="module14_cnt-box">
              {%- for block in section.blocks -%}
                <div class="module14_cnt-li">
                  {%- if section.settings.num != blank -%}
                    <div class="module14_li-m1">{{ section.settings.num }}</div>
                  {%- endif -%}
                  <div class="module14_li-m2">
                    <picture>
                      {%- if block.settings.image_mobile -%}
                        <source
                          media="(max-width: 767px)"
                          srcset="{{ block.settings.image_mobile | image_url: width: '400x' }} 400w, {{ block.settings.image_mobile | image_url: width: '600x' }} 600w, {{ block.settings.image_mobile | image_url: width: '800x' }} 800w, {{ block.settings.image_mobile | image_url: width: '1000x' }} 1000w"
                          width="{{ block.settings.image_mobile.width }}"
                          height="{{ block.settings.image_mobile.height }}"
                        >
                      {%- endif -%}
                      {%- if block.settings.image -%}
                        {{-
                          block.settings.image
                          | image_url: width: block.settings.image.width
                          | image_tag:
                            loading: 'eager',
                            sizes: '100vw',
                            widths: '300,400,500,600,700,800,900,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000,3200',
                            class: 'trade_banner__image'
                        -}}
                      {%- endif -%}
                    </picture>
                  </div>
                  {%- if block.settings.tle1 != blank -%}
                    <div class="module14_li-m3">{{ block.settings.tle1 }}</div>
                  {%- endif -%}
                  {%- if block.settings.tle2 != blank -%}
                    <div class="module14_li-m4">{{ block.settings.tle2 }}</div>
                  {%- endif -%}
                </div>
              {%- endfor -%}
            </div>
          </div>
          {%- if section.settings.note != blank -%}
            <div class="module_right-note">
              {{ section.settings.note }}
            </div>
          {%- endif -%}
        </div>
                <div class="module14_left to-top">
          <picture>
            {%- if section.settings.image_mobile -%}
              <source
                media="(max-width: 767px)"
                srcset="{{ section.settings.image_mobile | image_url: width: '400x' }} 400w, {{ section.settings.image_mobile | image_url: width: '600x' }} 600w, {{ section.settings.image_mobile | image_url: width: '800x' }} 800w, {{ section.settings.image_mobile | image_url: width: '1000x' }} 1000w"
                width="{{ section.settings.image_mobile.width }}"
                height="{{ section.settings.image_mobile.height }}"
              >
            {%- endif -%}
            {%- if section.settings.image -%}
              {{-
                section.settings.image
                | image_url: width: section.settings.image.width
                | image_tag:
                  loading: 'eager',
                  sizes: '100vw',
                  widths: '300,400,500,600,700,800,900,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000,3200',
                  class: 'trade_banner__image'
              -}}
            {%- endif -%}
          </picture>
        </div>
      </div>
    </div>
  </div>
</section>
<script></script>
{% schema %}
{
  "name": "Air10 Point Module14",
  "class": "shopify-section--air10-point-module14",
  "settings": [

       {
          "type": "image_picker",
          "id": "image",
          "label": "Images"
        },
     {
          "type": "image_picker",
          "id": "image_mobile",
          "label": "Mobile Images"
        },
     {
      "type": "richtext",
      "id": "tle_1",
      "label": "Text 1"
    },
     {
      "type": "richtext",
      "id": "note",
      "label": "Note"
    },
      {
          "type": "image_picker",
          "id": "product_image",
          "label": "Product Images"
        },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text color",
      "default": "rgba(0,0,0,0)"
    },
    {
      "type": "color",
      "id": "background",
      "label": "Background",
      "default": "rgba(0,0,0,0)"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 120,
      "step": 4,
      "unit": "px",
      "label": "t:sections.global.settings.padding_top.label",
      "default": 48
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 120,
      "step": 4,
      "unit": "px",
      "label": "t:sections.global.settings.padding_bottom.label",
      "default": 48
    }
  ],
  "blocks": [
        {
        "type": "item",
        "name": "item",
        "settings": [
           {
      "type": "text",
      "id": "num",
      "label": "Number"
    },
           {
          "type": "image_picker",
          "id": "image",
          "label": "Images"
        },
     {
          "type": "image_picker",
          "id": "image_mobile",
          "label": "Mobile Images"
        },
           {
      "type": "richtext",
      "id": "tle1",
      "label": "Text 1"
    },
          {
      "type": "richtext",
      "id": "tle2",
      "label": "Text 2"
    }
        ]
      }
  ],
  "presets": [
    {
      "name": "Air10 Module14"
    }
  ]
}
{% endschema %}
