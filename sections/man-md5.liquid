<style>
        .section-{{ section.id }}-padding {
    padding-top: calc({{ section.settings.padding_top }}px);
    padding-bottom: calc({{ section.settings.padding_bottom }}px);
  }
  @media screen and (max-width: 768px){
  .section-{{ section.id }}-padding {
    padding-top: calc({{ section.settings.padding_top }}px * 0.6);
    padding-bottom: calc({{ section.settings.padding_bottom }}px * 0.6);
  }
}
/* md5-box start*/
.md5-box{
  background: linear-gradient(277deg, #131618 1.99%, #1E252E 98.87%);
  position: relative;
}
.md5-box picture{
position: absolute;
top: 0;
right: 0;
width: 63vw;
    z-index: 1;
}
.md5-box picture img{
  width: 100%;
    object-fit: cover;
}
.md5_flex{
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
gap: 5vw;
}
.man_md5_left .man_tle1{
  color:  #FFF;
font-size: 50px;
font-style: normal;
font-weight: 500;
line-height: 130%;
}
.man_md5_left .man_tle2{
font-size: 70px;
font-style: normal;
font-weight: 500;
line-height: 130%;
background: linear-gradient(0deg, #B9E7FF 16.48%, #FFF 100%);
background-clip: text;
-webkit-background-clip: text;
-webkit-text-fill-color: transparent;
}
.man_md5_left .man_tle3{
  color: rgba(255, 255, 255, 0.50);
font-size: 14px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.man_md5_left .man_tle4{
  color:  #FFF;
text-align: center;
font-size: 24px;
font-style: normal;
font-weight: 400;
line-height: normal;
margin-left: 37px;
margin-top: 45px;
}
.man_md5-right{
  position: relative;
}
.man_md5-bg{
width: 30vw;
}
.circle_box img{
  width: 12vw;
}
.man_md5-right .circle_box{
  position: absolute;
  top: -10%;
    left: 16%;
}
.man_md5-right .circle_box:nth-of-type(2){
  top: 39%;
    left: -16%;
}
.man_md5-right .circle_box:nth-of-type(3){
  top: 80%;
    left: 30%;
}
.circle_pt {
  display: flex;
  align-items: center;
position: absolute;
width: 100%;
height: 100%;
inset: 0;
text-align: center;
justify-content: center;
flex-direction: column;
}
.circle_pt .circle_box-tle1{
  color: #a1a9ae;
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
}
.circle_pt .circle_box-tle2{
  color: #FFF;
/* font-size: 19px; */
font-size: 1.3vw;
font-style: normal;
font-weight: 400;
line-height: normal;
margin-top: 20px;
}
.circle_pt .circle_box-tle3{
/* font-size: 32px; */
font-size: 1.4vw;
font-style: normal;
font-weight: 500;
line-height: normal;
background: linear-gradient(0deg, #B9E7FF 0%, #FFF 100%);
background-clip: text;
-webkit-background-clip: text;
-webkit-text-fill-color: transparent;
}
@media (max-width: 992px) {
  .women_container{
    padding-left: 20px;
    padding-right: 20px;
  }
  .md5_flex{
    flex-direction: column;
    align-items: flex-start;
    gap: 100px;
  }
  .man_md5_left .man_tle1{
    font-size: 30px;
  }
  .man_md5_left .man_tle2{
    font-size: 67px;
    line-height: 1;
    margin-bottom:20px;
  }
  .man_md5_left .man_tle4{
    font-size: 16px;
    margin-left: 30px;
    margin-top: 10px;
    text-align: left;
  }
  .man_md5-bg{
    width: 93%;
    max-width: 390px;
  }
  .circle_box img{
    width: 129px;
  }
  .man_md5-right {
    text-align: right;
}
.man_md5-right .circle_box {
    left: 16%;
}
.man_md5-right .circle_box:nth-of-type(2) {
    left: -6%;
}
.man_md5-right .circle_box:nth-of-type(3) {
    left: 27%;
}
.circle_pt .circle_box-tle2{
  font-size: 12px;
  margin-top: 13px;
}
.circle_pt .circle_box-tle3{
  font-size: 17px;
}
.md5-box picture{
  width: 100%;
  top: 50%;
}
}
/* md5-box end*/
</style>
<section>
    <div class="md5-box section-{{ section.id }}-padding">
      <picture class="to-top" style="transition-delay: 0.5s">
        {%- if section.settings.image_mobile -%}
       <source
          media="(max-width: 767px)"
          srcset="{{ section.settings.image_mobile | image_url: width: '400x' }} 400w, {{ section.settings.image_mobile | image_url: width: '600x' }} 600w, {{ section.settings.image_mobile | image_url: width: '800x' }} 800w, {{ section.settings.image_mobile | image_url: width: '1000x' }} 1000w"
          width="{{ section.settings.image_mobile.width }}"
          height="{{ section.settings.image_mobile.height }}"
        >
        {%- endif -%}
        {%- if section.settings.image -%}
         {{- section.settings.image | image_url: width: section.settings.image.width | image_tag: loading: 'eager', sizes: '100vw', widths: '300,400,500,600,700,800,900,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000,3200', class: 'trade_banner__image' -}}
        {%- endif -%}
      </picture>
      <div class="women_container-cus">
        <div class="md5_flex">
          <div class="man_md5_left to-top">
            <div class="man_tle1">{{ section.settings.txt1 }}</div>
            <div class="man_tle2">{{ section.settings.txt2 }}</div>
            <div class="man_tle3">{{ section.settings.txt3 }}</div>
            <div class="man_tle4">
              {{ section.settings.txt4 }}
            </div>
          </div>
          <div class="man_md5-right to-top" style="transition-delay: 0.3s">
             {%- if section.settings.image_bg -%}
            <img src="{{ section.settings.image_bg | image_url }}" alt="" class="man_md5-bg">
             {%- endif -%}
            {%- for block in section.blocks -%}
            <div class="circle_box">
              {%- if block.settings.image -%}
              <img src="{{ block.settings.image |image_url }}" alt="">
              {%- endif -%}
              <div class="circle_pt">
                <div class="circle_box-tle1">{{ block.settings.text1 }}</div>
                <div class="circle_box-tle2">{{ block.settings.text2 }}</div>
                <div class="circle_box-tle3">{{ block.settings.text3 }}</div>
              </div>
            </div>
          {%- endfor -%}

          </div>
        </div>
      </div>
    </div>
  </section>
{% schema %}
{
  "name": "Man Module5",
  "class": "shopify-section--man-md5", 
  "settings": [
      {
          "type": "image_picker",
          "id": "image",
          "label": "Images"
        },
     {
          "type": "image_picker",
          "id": "image_mobile",
          "label": "Mobile Images"
        },
     {
          "type": "image_picker",
          "id": "image_bg",
          "label": "Background Images"
        },
     {
      "type": "text",
      "id": "txt1",
      "label": "Text1"
    },
     {
      "type": "text",
      "id": "txt2",
      "label": "Text2"
    },
     {
      "type": "text",
      "id": "txt3",
      "label": "Text3"
    },
     {
      "type": "richtext",
      "id": "txt4",
      "label": "Text4"
    },
         {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 300,
      "step": 4,
      "unit": "px",
      "label": "Padding Top",
      "default": 48
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 300,
      "step": 4,
      "unit": "px",
      "label": "Padding Bottom",
      "default": 48
    }
  ],
  "blocks": [
        {
      "type": "item",
      "name": "item",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
          {
      "type": "text",
      "id": "text1",
      "label": "Text1"
    },
        {
      "type": "richtext",
      "id": "text2",
      "label": "Text2"
    },
        {
      "type": "text",
      "id": "text3",
      "label": "Text3"
    }
      ]
    }
  ],
  "presets": [
    {
      "name": "Man Module5"
    }
  ]
}
{% endschema %}