<style>
/* md5__inner start*/
.md5-top{
  background: #E3D9D6;
  padding: 18.1vw 8vw;
}
.md5-top{
  display: flex;
    flex-direction: column;
    align-items: center;
}
.md5-tle{
  margin-bottom: 9.6vw;
}
.md5__inner img{
  width: 100%;
}
.md5-tle{
      font-family: var(--font-it-family);
  color: #3F3636;
font-size: 8vw;
font-style: normal;
font-weight: 600;
line-height: 130%;
letter-spacing: 0.598px;
text-transform: uppercase;
width: 100%;
}
.md5-tle-img2{
  text-align: right;
}
.md-top-img{
width: 70.1vw;
}
.md5-bt{
  padding: 0px 5.3vw 5.3vw;
}
.md5__inner img.md5_bt-timg{
  width: 54vw;
}
.md5_bt-tle{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.md5_bt-tle svg{
  transform: translateY(-40px);
}


.md_pd-inner{
  padding-bottom: 17.33vw;
}
.md_pd-bg{
  background: #DE004D;
  color:  #FFF;
text-align: center;
font-size: 5.4vw;
font-style: normal;
font-weight: 500;
line-height: 180%;
letter-spacing: 1px;
}
.md_pd-bg strong{
  font-family: var(--font-it-family);
 font-style: normal;
font-weight: 500;
}
.detailBox__item{
  position: relative;
    padding-top: 12.6vw;
    padding-left: 7.2vw;
    margin-left: 15.5vw;
    z-index: 2;
}
.detailBox__item__title .jp{
  font-family: var(--font-it-family);
  color: #3F3636;
font-size: 5.6vw;
font-style: normal;
font-weight: 400;
line-height: 150%;
}
.detailBox__item__title .eng{
  color: #3F3636;
font-size: 3.7vw;
font-style: normal;
font-weight: 400;
line-height: 130%;
}
.detailBox__item__title{
  margin-bottom: 5.6vw;
}
.detailBox__item:before {
    content: "";
    width: 0.5px;
    height: calc(100% + 30px);
    background-color: #3F3636;
    position: absolute;
    top: -30px;
    left: 0;
    transition: height 1.5s cubic-bezier(.25,1,.5,1);
}
.detailBox__item__price {
  display: flex;
  align-items: flex-end;
  gap: 2.6vw;
}
.detailBox__item__price1{
    font-family: var(--font-it-family);
  color:  #3F3636;
font-size: 6.4vw;
font-style: normal;
font-weight: 500;
line-height: 130%;
}
.detailBox__item__price1 strong{
        font-family: var(--font-body-family);
  font-size: 3.2vw;
font-style: normal;
font-weight: 500;
line-height: 130%;
}
.detailBox__item__price2{
      font-family: var(--font-it-family);
  color: #3F3636;
font-size: 3.2vw;
font-style: normal;
font-weight: 400;
line-height: 1.6;
letter-spacing: -0.48px;
}
  .detailBox__item__price2 em{
    font-style: normal;
font-weight: 400;
    text-decoration: line-through;
  }
.detailBox__item__price2 strong{
  font-family: var(--font-body-family);
  color: #3F3636;
font-size: 1.8vw;
font-style: normal;
font-weight: 500;
line-height: 130%;
text-decoration: line-through;
}
.detailBox__item__type{
  color: #3F3636;
font-size: 3.4vw;
font-style: normal;
font-weight: 300;
line-height: 150%;
margin-bottom: 5.6vw;
}
.md5-top-two{
  position: relative;
}
.md5_two-txt{
      font-family: var(--font-it-family);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: absolute;
  width: 100%;
  height: 100%;
  inset: 0;
  color: #FFF;
text-align: center;
font-size: 8.8vw;
font-style: normal;
font-weight: 600;
line-height: 150%;
letter-spacing: 2.746px;
text-transform: uppercase;
}
    .md5_bt-tle img{
    width:53.3vw !important;
  }
.atm_md5 .md-top-img{
  width: 68.26666666666667vw;
}
 .atm_md5 .md5-top {
    align-items: flex-end;
}
  .atm_md5 .md5-tle {
    font-weight: 400;
    position: absolute;
    left: 8%;
    z-index: 1;
    top: 24%;
}
   .atm_md5 .md5-tle strong{
     font-weight: 600;
     font-style: normal;
   }
  .md5__inner.atm_md5 svg{
        position: absolute;
    width: auto;
    bottom: 30px;
    left: 0;
  }
@media only screen and (min-width: 768px) {
  .md5-top {
    padding: 90px 40px;
}
.md5-tle {
    font-size: 40px;
    margin-bottom: 48px;
}
.md-top-img {
    width: 350px;
}
.md5-bt {
    padding: 0px 27px 27px
}
.md5__inner img.md5_bt-timg {
    width: 270px;
}
.md_pd-inner {
    padding-bottom: 87px
}
.detailBox__item {
    padding-top: 63px;
    padding-left: 36px;
    margin-left: 78px;
}
.detailBox__item__title {
    margin-bottom: 28px;
}
.detailBox__item__title .jp {
    font-size: 28px;
}
.detailBox__item__title .eng {
    font-size: 18px;
}
.detailBox__item__type {
    font-size: 17px;
    margin-bottom: 28px;
}
.detailBox__item__price {
    gap: 13px;
}
.detailBox__item__price1 {
    font-size: 32px;
}
.detailBox__item__price1 strong {
    font-size: 16px;
}
.detailBox__item__price2 {
    font-size: 16px;
}
.detailBox__item__price2 strong {
    font-size: 9px;
}
.md5_two-txt {
    font-size: 44px;
}
    .md5_bt-tle img{
    width:266px!important;
  }
  .md_pd-bg{
        font-size: 26px;
  }
  .atm_md5 .md-top-img{
  width: 340px;
}
}
/* md5__inner end*/
</style>
  <section>
    <div class="md5__inner atm_md5">
      <div class="md5-top">
         {%- if section.settings.show_svg -%}
        <svg xmlns="http://www.w3.org/2000/svg" width="298" height="127" viewBox="0 0 298 127" fill="none">
          <path d="M297 1C267.833 55.3333 147 184.5 33.5 95.5C12 73.5 29.5 50.5 40.5 63C51.5 75.5 46.5 156 -67 104.5" stroke="#EB9A73" class="atm_path"/>
        </svg>
        {%- endif -%}
        <div class="md5-tle">
          <div class="md5-tle-img1 to-top">{{ section.settings.txt_1 }}</div>
          <div class="md5-tle-img2 to-top">{{ section.settings.txt_2 }}</div>
        </div>
        <div class="md-top-img scale_img" style="transition-delay: 105ms;">
          {%- if section.settings.image -%}
         <img src="{{ section.settings.image | image_url }}" alt="">
        {%- endif -%}
        </div>
      </div>
     
    </div>
  </section>
{% schema %}
{
  "name": "Atm Md5",
  "class": "shopify-section--brand-md5", 
  "settings": [
     {
          "type": "checkbox",
          "id": "show_svg",
          "label": "Show Svg",
         "default":false
        },
    {
          "type": "image_picker",
          "id": "image",
          "label": "Images"
        },
     {
      "type": "richtext",
      "id": "txt_1",
      "label": "Text 1"
    },
    {
      "type": "richtext",
      "id": "txt_2",
      "label": "Text 2"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text color",
      "default": "rgba(0,0,0,0)"
    },
    {
      "type": "color",
      "id": "background",
      "label": "Background",
      "default": "rgba(0,0,0,0)"
    }
  ],
  "blocks": [
  ],
  "presets": [
    {
      "name": "Atm Md5"
    }
  ]
}
{% endschema %}