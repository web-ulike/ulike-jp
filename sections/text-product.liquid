 {%- liquid
  assign product = section.settings.product
-%}
<section>
  {%- for option in product.options_with_values -%}
  <div style="height:100px">
    {%- for value in option.values -%}

      <div>{{ value }}</div>
      
    {%- endfor -%}
  </div>
     {%- endfor -%}
</section>
<script>
   window.aa = product
</script>
{% schema %}
{
  "name": "man-sell-point-model2",
  "settings": [
    {
      "type": "product",
      "id": "product",
      "label": "Product"
    }
  ],

  "presets": [
    {
      "name": "Test Product"
    }
  ]
}
{% endschema %}
