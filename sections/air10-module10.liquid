<style>
    #shopify-section-{{ section.id }} {
    --text-color: {{  section.settings.text_color.red }}, {{  section.settings.text_color.green }}, {{  section.settings.text_color.blue }};
    --section-background: {{ section.settings.background.red }}, {{ section.settings.background.green }}, {{ section.settings.background.blue }};
    --color-border: 255, 255, 255;
    /* background: rgb(var(--section-background, var(--background))); */
  }
  .section-{{ section.id }}-padding {
    padding-top: calc({{ section.settings.padding_top }}px);
    padding-bottom: calc({{ section.settings.padding_bottom }}px);
  }
  @media screen and (max-width: 768px){
  .section-{{ section.id }}-padding {
    padding-top: calc({{ section.settings.padding_top }}px * 0.6);
    padding-bottom: calc({{ section.settings.padding_bottom }}px * 0.6);
  }
}
.module10_md1{
  max-width: 764px;
  margin: 0 auto;
}
.module10_md1 .module10_md1-tle{
  color: #000;
  font-family: var(--font-hd-family);
  font-size: 30px;
  font-style: normal;
  font-weight: 400;
  line-height: 150%;
  letter-spacing: 0.3px;
  margin: 74px 0;
}
.module10_md2{
  margin-top: 72px;
  background: #F9F8F6;
  padding: 60px;
  position: relative;
  display: flex;
  justify-content: space-between;
}
.module10_md2-right{
  align-self: flex-end;
  color: rgba(0, 0, 0, 0.50);
font-size: 12px;
font-style: normal;
font-weight: 400;
line-height: 180%;
letter-spacing: 0.12px;
position: relative;
z-index: 1;
}
.module10_md2-img {
  position: absolute;
  top: -34%;
  right: -19%;
}
.module10_md2 .module10_md2-left{
width: 50%;
position: relative;
z-index: 1;
}
.module10_md2 .module10_md2-left .module10_md2-txt1{
  color: #000;
font-family: var(--font-hd-family);
font-size: 30px;
font-style: normal;
font-weight: 400;
line-height: 150%;
letter-spacing: 1.5px;
}

.module10_md2 .module10_md2-left .module10_md2-txt2{
  color: #000;
  font-size: 16px;
  font-style: normal;
  font-weight: 400;
  line-height: 180%;
  letter-spacing: 0.18px;
  margin-top: 50px;
}
.module10_md2 .module10_md2-left .module10_md2-txt3{
  color: #000;
font-size: 16px;
font-style: normal;
font-weight: 400;
line-height: 180%;
letter-spacing: 0.15px;
margin-top: 150px;
}
.module10_md2-img{
  position: absolute;
}
  .destok_hidden{
  display:none;
}
@media (max-width: 767px){
  .destok_hidden{
  display:block;
}
  .mobile_hidden{
    display:none;
  }
  .module10_md1 .module10_md1-tle{
    font-size: 18px;
    margin: 30px 0;
  }
  .module10_md2{
    padding: 50px 20px;
    flex-direction: column;
    margin: 20px -20px 0;
  }
  .module10_md2 .module10_md2-left{
    width: 100%;
  }
  .module10_md2 .module10_md2-left .module10_md2-txt1{
    font-size: 18px;
  }
  .module10_md2 .module10_md2-left .module10_md2-txt2{
    font-size: 14px;
    margin-top: 20px;
  }
  .module10_md2 .module10_md2-left .module10_md2-txt3{
    margin-top: 20px;
    font-size: 14px;
  }
  .module10_md2-img{
    position: relative;
    top: 0;
    left: 0;
  }
  .module10_md2-right{
    position: absolute;
    bottom: 30px;
    right: 20px;
    font-size: 10px;
  }
  .module10_md1 picture{
    display: flex;
    justify-content: flex-end;
  }
  .module10_md1 picture img{
    width: 80%;
  }
}
</style>
   <section>
    <div class="air_module10_wrapper section-{{ section.id }}-padding">
      <div class="custom_container">
        <div class="module10_md1">
          <picture class="to-top">
            {%- if section.settings.image_mobile -%}
           <source
              media="(max-width: 767px)"
              srcset="{{ section.settings.image_mobile | image_url: width: '400x' }} 400w, {{ section.settings.image_mobile | image_url: width: '600x' }} 600w, {{ section.settings.image_mobile | image_url: width: '800x' }} 800w, {{ section.settings.image_mobile | image_url: width: '1000x' }} 1000w"
              width="{{ section.settings.image_mobile.width }}"
              height="{{ section.settings.image_mobile.height }}"
            >
            {%- endif -%}
            {%- if section.settings.image -%}
             {{- section.settings.image | image_url: width: section.settings.image.width | image_tag: loading: 'eager', sizes: '100vw', widths: '300,400,500,600,700,800,900,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000,3200', class: 'trade_banner__image' -}}
            {%- endif -%}
          </picture>
          {%- if section.settings.tle_1 != blank -%}
          <div class="module10_md1-tle to-top" style="transition-delay: 0.1s;">
            {{ section.settings.tle_1 }}
          </div>
           {%- endif -%}
          <svg xmlns="http://www.w3.org/2000/svg" width="27" height="106" viewBox="0 0 27 106" fill="none" class="to-top mobile_hidden" style="transition-delay: 0.2s;">
            <path d="M13.4887 105.305C13.2754 105.305 13.0638 105.226 12.9021 105.075L-9.27577e-05 93.0192L1.17315 91.7602L12.6286 102.464L12.6286 -7.51965e-08C13.5782 -3.36881e-08 14.3489 0.771553 14.3489 1.72222L14.3489 104.405C14.3489 104.73 14.1854 105.047 13.8982 105.202C13.7691 105.273 13.6281 105.305 13.4887 105.305Z" fill="#E0004D"/>
            <path d="M16.5045 103.504C15.8336 102.831 15.8336 101.741 16.5045 101.069L25.782 91.7793L26.9983 92.9969L16.5045 103.502L16.5045 103.504Z" fill="#E0004D"/>
          </svg>
          <svg xmlns="http://www.w3.org/2000/svg" width="14" height="58" viewBox="0 0 14 58" fill="none" class="to-top destok_hidden" style="transition-delay: 0.2s;">
            <path d="M7.24442 57.6527C7.13777 57.6527 7.03197 57.6131 6.95111 57.5373L0.500014 51.5096L1.08663 50.8801L6.81435 56.2319L6.81435 -3.75983e-08C7.28915 -1.68441e-08 7.6745 0.385777 7.6745 0.861109L7.6745 57.2024C7.6745 57.3651 7.59278 57.5236 7.44914 57.6011C7.38463 57.6364 7.3141 57.6527 7.24442 57.6527Z" fill="#E0004D"/>
            <path d="M8.75213 56.7521C8.41667 56.4154 8.41667 55.8703 8.75213 55.5345L13.3909 50.8896L13.999 51.4985L8.75213 56.7512L8.75213 56.7521Z" fill="#E0004D"/>
          </svg>
        </div>
        <div class="module10_md2 to-top" style="transition-delay: 0.3s;">
          <div class="module10_md2-left">
            {%- if section.settings.tle_2 != blank -%}
            <div class="module10_md2-txt1">
             {{ section.settings.tle_2 }}
            </div>
            {%- endif -%}
            <div class="module10_md2-txt2">{{ section.settings.tle_3 }}</div>
            <div class="module10_md2-txt3">{{ section.settings.tle_4 }}</div>
          </div>
          {%- if section.settings.note != blank -%}
          <div class="module10_md2-right">
            {{ section.settings.note }}
          </div>

          {%- endif -%}

           <picture class="module10_md2-img">
            {%- if section.settings.product_image -%}
           <source
              media="(max-width: 767px)"
              srcset="{{ section.settings.product_image | image_url: width: '400x' }} 400w, {{ section.settings.product_image | image_url: width: '600x' }} 600w, {{ section.settings.product_image | image_url: width: '800x' }} 800w, {{ section.settings.product_image | image_url: width: '1000x' }} 1000w"
              width="{{ section.settings.product_image.width }}"
              height="{{ section.settings.product_image.height }}"
            >
            {%- endif -%}
            {%- if section.settings.product_image -%}
             {{- section.settings.product_image | image_url: width: section.settings.product_image.width | image_tag: loading: 'eager', sizes: '100vw', widths: '300,400,500,600,700,800,900,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000,3200', class: '' -}}
            {%- endif -%}
          </picture>
        </div>
      </div>
    </div>
  </section>
<script>
</script>
