<link rel="stylesheet" href="{{ 'swiper-bundle.min.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'swiper-bundle.min.css' | asset_url | stylesheet_tag }}</noscript>
<style>
#shopify-section-{{ section.id }} {
    background-color: {{ section.settings.background }}; /* Allows to set a placeholder color while loading */
}
.section-{{ section.id }}-padding {
    padding-top: calc({{ section.settings.padding_top }}px * 0.75);
    padding-bottom: calc({{ section.settings.padding_bottom }}px  * 0.75);
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
.swiper-fb-box .swiper_extend_box{
  position:relative;
}
.swiper-fb-box .swiper-button-prev-custom, .swiper-rtl .swiper-button-next-custom {
    left: -54px;
}
.swiper-fb-box .swiper-button-next-custom, .swiper-rtl .swiper-button-prev-custom {
    right: -54px;
}
.swiper-fb-box .swiper-button-next-custom, .swiper-fb-box .swiper-button-prev-custom {
   position: absolute;
    top: 50%;
    width: 40px;
    height: 40px;
    margin-top: 0px;
    z-index: 10;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    transform: translate(0%, -50%);
}

.swiper-fb-box{
  position:relative;
}
.swiper-fb-box .swiper-pagination{
  position: static;
  margin-top: 30px;
}
.swiper-fb-box-extend{
  padding-left:10rem;
  padding-right:10rem;
}
.swiper-fb-box .swiper-pagination-bullet{
  width: 13px;
  height: 13px;
}
.swiper-fb-box .swiper-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet, .swiper-fb-box .swiper-pagination-horizontal.swiper-pagination-bullets .swiper-pagination-bullet {
   margin: 0 5px;
}
.swiper-fb-box .swiper-pagination-bullet-active {
    background: #181818;
}
.swiper-fb-box .h2{
  text-align: center;
  /*font-size:34px;*/
}
.swiper-fb-box .h3{
  text-align: center;
  font-size: 32px;
  margin-top:0;
}
.swiper-fb-box .des{
text-align: center;
  font-size: 18px;
  margin-bottom:45px;
  line-height: 1.3;
}
.swiper-fb-box .why-list-extend {
    padding: 12.5px 15px;
    margin-top: 25px;
    background: #ffffff;
}
.swiper-fb-box .why-box .mt30 {
    margin-bottom: 30px;
}
.swiper-fb-box .why-list-extend .start svg {
    display:inline-block;
    height: 14px;
    width:95px;
}
.swiper-fb-box .why-list-extend .des {
    font-size: 16px;
    height: 196px;
    line-height: 1.3;
    padding-top: 15px;
    text-align: left;
    margin-bottom: 0;
    max-height: 196px;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 8;
    line-clamp: 8;
    -webkit-box-orient: vertical;
    margin-top: 0;
}
.swiper-fb-box .why-list-extend .line {
    width: 40px;
    height: 1px;
    background: #E6E6E6;
}
.swiper-fb-box .why-list-extend .name {
    font-size: 26px;
    font-family: 'soehne-kraftig';
    line-height: 1.2;
    margin-bottom: 3px;
}
.swiper-fb-box .why-list-extend .start {
    margin-bottom: 10px;
    text-align: left;
}
.bg-extend{
  background: #f7f7f7;
  padding: 100px 0;
}
.swiper-fb-box .check_box{
  max-width:1350px;
}
.swiper-fb-box-extend.swiper-fb-box .swiper-button-next-custom, .swiper-fb-box-extend.swiper-fb-box .swiper-button-prev-custom{
  margin-top: -25px;
}
.swiper-fb-box .img-box-extend .swiper-fb__image{
  height: 293px;
}
.swiper-fb-box-extend .img-box-extend .swiper-fb__image{
  height: 544px;
}
.swiper-fb-box .img-box-extend.img-box-extend-position {
  display: flex;
    flex-direction: column-reverse;
    align-items: start;
    justify-content: space-between;
}
.swiper-fb-box .img-box-extend.img-box-extend-position .why-list-extend{
  margin-top: 0;
  margin-bottom: 25px;
}
.swiper-fb-box .swiper-button-next-custom svg.design-iconfont, .swiper-fb-box .swiper-button-prev-custom svg.design-iconfont {
    display: inline-block;
    width: 40px;
    height: 40px;
}
@media screen and (max-width: 768px){
.swiper-fb-box .swiper-button-prev-custom{
  left: 0px;
}
.swiper-fb-box .swiper-button-next-custom{
  right:0px;
}
.swiper-fb-box-extend{
  padding-left: 5%;
    padding-right: 5%;
}
.swiper-fb-box .img-box-extend .swiper-fb__image{
  height: auto;
}
.swiper-fb-box-extend .img-box-extend .swiper-fb__image{
  height: auto;
}
.swiper-fb-box .img-box-extend.img-box-extend-position{
  flex-direction: column;
}
.swiper-fb-box .img-box-extend.img-box-extend-position .why-list-extend {
    margin-top: 25px;
    margin-bottom: 0;
}
}
</style>
<div class="container swiper-fb-box{% if section.settings.model==true %} swiper-fb-box-extend{% endif %} section-{{ section.id }}-padding">
  {%- if section.settings.title != blank -%}
    <h2 class="title h2">{{ section.settings.title | escape }}</h2>
  {%- endif -%}
  {%- if section.settings.subTitle != blank -%}
    <h3 class="title h3">{{ section.settings.subTitle | escape }}</h3>
  {%- endif -%}
  {%- if section.settings.description != blank -%}
  <div class="des des-txt">{{ section.settings.description | escape }}</div>
  {%- endif -%}
  <div class="swiper_extend_box">
    <div class="swiper mySwiper" id="swiper--{{ section.id }}">
      <div class="swiper-wrapper">
        {%- for block in section.blocks -%}
        {%- assign image = block.settings.image -%}
        {%- assign mobile_image = block.settings.image_mobile -%}
        <div class="swiper-slide" style="width:auto">
          <div class="img-box-extend {% if block.settings.check_img_position==true %} img-box-extend-position{% endif %}">
              <div class="{% if mobile_image != blank %}hidden-pocket{% endif %}">
                {%- if image != blank -%}
                  {{- image | image_url: width: 3000 | image_tag: loading: 'lazy', draggable: false, class: 'swiper-fb__image', sizes: '100vw', widths: '600,700,800,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000' -}}
                {%- endif -%}
              </div>
              {%- if mobile_image != blank -%}
                <div class="hidden-lap-and-up">
                  {{- mobile_image | image_url: width: 3000 | image_tag: loading: 'lazy', draggable: false, class: 'swiper-fb__image', sizes: '100vw', widths: '600,700,800,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000' -}}
                </div>
              {%- endif -%}
            {%- if block.settings.name != blank -%}
            <div class="why-list-extend mt30">
              <div class="name">
                {{ block.settings.name | escape }}
              </div>
              <div class="start">
                <svg fill="none" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 95 14" class="design-iconfont">
                  <path d="M7.09934 0L9.19227 4.58407L14.1987 5.15794L10.4857 8.56499L11.487 13.5038L7.09934 11.0254L2.71167 13.5038L3.71294 8.56499L0 5.15794L5.0064 4.58407L7.09934 0Z" fill="#EBBF1F"></path>
                  <path d="M47.0993 0L49.1923 4.58407L54.1987 5.15794L50.4857 8.56499L51.487 13.5038L47.0993 11.0254L42.7117 13.5038L43.7129 8.56499L40 5.15794L45.0064 4.58407L47.0993 0Z" fill="#EBBF1F"></path>
                  <path d="M27.0993 0L29.1923 4.58407L34.1987 5.15794L30.4857 8.56499L31.487 13.5038L27.0993 11.0254L22.7117 13.5038L23.7129 8.56499L20 5.15794L25.0064 4.58407L27.0993 0Z" fill="#EBBF1F"></path>
                  <path d="M67.0993 0L69.1923 4.58407L74.1987 5.15794L70.4857 8.56499L71.487 13.5038L67.0993 11.0254L62.7117 13.5038L63.7129 8.56499L60 5.15794L65.0064 4.58407L67.0993 0Z" fill="#EBBF1F"></path>
                  <path d="M87.0993 0L89.1923 4.58407L94.1987 5.15794L90.4857 8.56499L91.487 13.5038L87.0993 11.0254L82.7117 13.5038L83.7129 8.56499L80 5.15794L85.0064 4.58407L87.0993 0Z" fill="#EBBF1F"></path>
                </svg>
              </div>
              <div class="line">
              </div>
              <div class="des">
                {{ block.settings.description1 | escape }}
              </div>
            </div>
            {%- endif -%}
          </div>
        </div>
        {%- endfor -%}
      </div>
      <div class="swiper-pagination swiper-pagination--{{ section.id }}"></div>
    </div>
    <div class="swiper-button-prev-custom swiper-button-prev-custom--{{ section.id }}">
      <svg fill="none" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 40 40" class="design-iconfont">
        <mask id="fo23klz2p__3cpv3dnfha" style="mask-type:alpha" maskUnits="userSpaceOnUse" x="5" y="4" width="31" height="31">
          <path transform="matrix(-1 0 0 1 36 4)" fill="#D9D9D9" d="M0 0H31V31H0z"/>
        </mask>
        <g mask="url(#fo23klz2p__3cpv3dnfha)">
          <path d="M25.458 3.45751L9.37303 19.5425L25.458 35.6274" stroke="#fff" stroke-width="1.5"/>
        </g>
        <mask id="fo23klz2p__j8h2lhv24b" style="mask-type:alpha" maskUnits="userSpaceOnUse" x="4" y="3" width="33" height="33">
          <path transform="matrix(-1 0 0 1 37 3)" fill="#D9D9D9" d="M0 0H33V33H0z"/>
        </mask>
        <g mask="url(#fo23klz2p__j8h2lhv24b)">
          <path d="M24.4582 2.45776L7.37352 19.5425L24.4582 36.6272" stroke="#040000" stroke-width="1.5"/>
        </g>
      </svg>
    </div>
    <div class="swiper-button-next-custom swiper-button-next-custom--{{ section.id }}">
      <svg fill="none" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 40 40" class="design-iconfont">
        <mask id="f6v4xaqwh__fqcktyk72a" style="mask-type:alpha" maskUnits="userSpaceOnUse" x="4" y="4" width="31" height="31">
          <path fill="#D9D9D9" d="M4 4H35V35H4z"/>
        </mask>
        <g mask="url(#f6v4xaqwh__fqcktyk72a)">
          <path d="M14.542 3.45751L30.627 19.5425L14.542 35.6274" stroke="#fff" stroke-width="1.5"/>
        </g>
        <mask id="f6v4xaqwh__ij7zspkldb" style="mask-type:alpha" maskUnits="userSpaceOnUse" x="3" y="3" width="33" height="33">
          <path fill="#D9D9D9" d="M3 3H36V36H3z"/>
        </mask>
        <g mask="url(#f6v4xaqwh__ij7zspkldb)">
          <path d="M15.5418 2.45776L32.6265 19.5425L15.5418 36.6272" stroke="#040000" stroke-width="1.5"/>
        </g>
      </svg>
    </div>
  </div>
</div>
<script src="{{ 'swiper-bundle.min.aio.min.js' | asset_url }}"></script>
<script>
(function(){
  let documentWidth = document.documentElement.clientWidth || document.body.clientWidth;
  let num1 =4,num2=5;
  if(documentWidth < 769 ) { 
    num1 =1,num2=1;
  }
  if(Shopify.designMode || {{ section.settings.model }}){
    let gallerySwiper = new Swiper('#swiper--{{ section.id }}',{
      spaceBetween: 57,
      slidesPerView: num1,
      pagination: {
          el: ".swiper-pagination--{{ section.id }}",
          clickable: true,
        },
      navigation: {
          nextEl: '.swiper-button-next-custom--{{ section.id }}',
          prevEl: '.swiper-button-prev-custom--{{ section.id }}'
      }
    })
  } else {
    let gallerySwiper = new Swiper('#swiper--{{ section.id }}',{
      spaceBetween: 14,
      slidesPerView: num2,
      navigation: {
          nextEl: '.swiper-button-next-custom--{{ section.id }}',
          prevEl: '.swiper-button-prev-custom--{{ section.id }}'
      }
    })
  }
  })()
</script>
{% schema %}
{
  "name": "Swiper FB",
  "tag": "section",
  "class": "section",
  "max_blocks": 20,
  "settings": [
    {
      "type": "checkbox",
      "id": "model",
      "default": false,
      "label": "Model"
    },
    {
      "type": "text",
      "id": "title",
      "default": "Swiper FB",
      "label": "Heading"
    },
    {
      "type": "text",
      "id": "subTitle",
      "default": "Swiper FB",
      "label": "SubHeading"
    },
    {
      "type": "text",
      "id": "description",
      "default": "Swiper FB",
      "label": "Description"
    },
     {
      "type": "color",
      "id": "background",
      "label": "Background",
      "info": "Used while slideshow image is loading",
      "default": "rgba(0,0,0,0)"
    },
     {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Padding Top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Padding Bottom",
      "default": 36
    }
  ],
  "blocks": [
      {
        "type": "logo",
        "name": "image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          },
          {
            "type": "checkbox",
            "id": "check_img_position",
            "default": false,
            "label": "Check Image Position"
          },
          {
            "type": "image_picker",
            "id": "image_mobile",
            "label": "Mobile Image"
          },
          {
            "type": "text",
            "id": "name",
            "default": "Kelly",
            "label": "Name"
          },
          {
            "type": "text",
            "id": "description1",
            "default": "I have sensitive skin and am afraid of hair removal with wax, which is painful and makes the skin red and itchy. So I bought Ulike with the idea of giving......",
            "label": "Description"
          },
          {
            "type": "range",
            "id": "start_num",
            "min": 0,
            "max": 5,
            "step": 1,
            "unit": "px",
            "label": "Start Num",
            "default": 5
          }
        ]
      }
    ],
  "presets": [
      {
        "name": "Swiper FB",
        "blocks": [
          {
            "type": "logo"
          },
          {
            "type": "logo"
          },
          {
            "type": "logo"
          },
          {
            "type": "logo"
          }
        ]
      }
    ]
}
{% endschema %}