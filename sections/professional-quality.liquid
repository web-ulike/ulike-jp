<script>
  (function () {
    // 函数来设置行高
    function setRowHeights() {
      // 获取所有列
      const columns = document.querySelectorAll('.professional-quality-column');

      // 如果没有列则不执行
      if (columns.length === 0) return;

      // 初始化一个数组来存储每一行的最大高度
      const maxHeights = [];
      // 首先遍历所有列，计算每一行的最大高度
      columns.forEach((column) => {
        const rows = column.querySelectorAll('.professional-quality-item');
        Array.from(rows).forEach((row, index) => {
          const rowHeight = row.offsetHeight;

          if (!maxHeights[index] || rowHeight > maxHeights[index]) {
            maxHeights[index] = rowHeight;
          }
        });
      });

      // 再次遍历列，将每一列的行高度设置为该行的最大高度
      columns.forEach((column) => {
        const rows = column.querySelectorAll('.professional-quality-item');
        Array.from(rows).forEach((row, index) => {
          row.style.height = `${maxHeights[index]}px`;
        });
      });
    }

    // 函数来处理窗口大小变化
    function onResize() {
      // 清除之前的行高设置
      const columns = document.querySelectorAll('.professional-quality-column');
      columns.forEach((column) => {
        const rows = column.children;
        Array.from(rows).forEach((row) => {
          row.style.height = ''; // 重置高度
        });
      });

      // 重新设置行高
      setRowHeights();
    }

    // 初始化
    window.addEventListener('load', setRowHeights);
    window.addEventListener('resize', onResize);
    function onImagesLoaded(imageElements, callback) {
      const promises = Array.from(imageElements).map((img) => {
        return new Promise((resolve, reject) => {
          if (img.complete) {
            // 如果图片已经加载完成
            resolve();
          } else {
            // 如果图片尚未加载，监听加载事件
            img.onload = resolve;
            img.onerror = reject;
          }
        });
      });

      // 当所有图片都加载完毕时，执行回调
      Promise.all(promises)
        .then(callback)
        .catch((error) => {
          console.error('Some images failed to load', error);
        });
    }
    onImagesLoaded(document.querySelectorAll('.professional-quality-column img'), () => {
      onResize();
      // 在此处执行你的事件或操作
    });
  })();

  window.addEventListener('load', function () {
    function professionalQualitySwiper() {
      let $p = $('.professional-quality');
      let $right = $p.find('.professional-quality-right');
      let $list = $p.find('.professional-quality-cont');
      let $item = $p.find('.professional-quality-other');
      let $arraw = $p.find('.professional-quality-arraw-item');
      let length = $item.length;
      let width = $item.width();
      let index = 0;
      $arraw.eq(1).css('opacity', '1');
      function changeSwiper() {
        $list.css({
          transform: `translateX(-${index * width}px)`,
          transition: 'all .5s cubic-bezier(0.645, 0.045, 0.355, 1)',
        });
      }
      $arraw.click(function () {
        let sindex = $(this).index();
        if (sindex && index < length - 1) index++;
        if (!sindex && index > 0) index--;
        if (index != length - 1) {
          $arraw.eq(1).css('opacity', '1');
        } else {
          $arraw.eq(1).css('opacity', '.3');
        }
        if (index != 0) {
          $arraw.eq(0).css('opacity', '1');
        } else {
          $arraw.eq(0).css('opacity', '.3');
        }
        changeSwiper();
      });
    }
    if (window.innerWidth < 461) professionalQualitySwiper();
  });
</script>
