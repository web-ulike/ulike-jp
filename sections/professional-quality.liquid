<style>
  .professional-quality {
    padding: 110px 0 90px 0;
  }

  .professional-quality .professional-quality-title {
      font-family: var(--heading-font-family);
    color: #000;
    text-align: center;
    font-size: 48px;
    line-height: 1.25;
  }

  .professional-quality .professional-quality-sub {
    color: #3f3636;
    text-align: center;
    font-size: 18px;
    line-height: 1.58;
    margin-top: 15px;
  }

  .professional-quality .professional-quality-flex {
    display: flex;
    margin-top: 60px;
    gap: 20px;
  }

  .professional-quality .professional-quality-left {
    width: 39%;
    display: flex;
    position: relative;
  }

  .professional-quality .professional-quality-column {
    flex: 1;
    padding: 30px 0 0;
    position: relative;
    z-index: 1;
  }
  .professional-quality .professional-quality-icon {
    width: 100%;
    position: relative;
  }

  .professional-quality .professional-quality-tag {
    width: 36%;
    max-width: 80px;
    position: absolute;
    top: 50%;
    transform: translate(calc(50% + 22px), -50%);
    right: 0;
  
  }

  .professional-quality .professional-quality-tag img {
    display: block;
    width: 100%;
  }

  .professional-quality .icon {
    display: block;
    width: 43.5%;
    max-width: 115px;
    margin: 0 auto;
  }

  .professional-quality .professional-quality-item svg {
    display: block;
  }

  .professional-quality .professional-quality-item {
    padding: 24px 15px;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    text-align: center;
    position: relative;
    font-family: var(--heading-font-family);
  }

  .professional-quality .professional-quality-head {
    color: #190a0a;
    font-size: 18px;
    line-height: 1.1;
    font-family: var(--heading-font-family);
  }

  .professional-quality .professional-quality-head .professional-quality-item {
    position: relative;
        padding: 27px 50px;
    text-align: left;
    align-items: flex-start;
  }

  .professional-quality .professional-quality-head .professional-quality-item::before {
    content: '';
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    width: calc(100vw);
    border-bottom: 1px solid #d9d9d9;
    max-width:1520px;
  }

  .professional-quality .professional-quality-fixed {
    color: #e0004d;
    font-size: 22px;
    line-height: 1.25;
    border-radius: 10px;
    background: linear-gradient(180deg, #FDF4FF 0%, #FEFAFF 100%);
    position: relative;
    z-index: 1;
  }

  .professional-quality .professional-quality-fixed .professional-quality-item:last-child {
    font-size: 32px;
    font-family: var(--heading-font-family);
  }

  .professional-quality .professional-quality-text {
    margin-top: 19px;
    font-size: 28px;
    font-family: var(--text-font-family);
  }

  .professional-quality .professional-quality-right {
    overflow: hidden;
    border-radius: 20px;
    flex: 1;
  }

  .professional-quality .professional-quality-cont {
    display: flex;
    height: 100%;
  }

  .professional-quality .professional-quality-other {
    color: #190a0a;
    font-size: 22px;
    line-height: 1.25;
    /* max-width: 20%; */
  }

  .professional-quality .professional-quality-other .professional-quality-item:last-child {
    font-size: 32px;
  }

  .professional-quality .professional-quality-sm {
    font-size: 14px;
    line-height: 1.2;
  }

  .professional-quality .professional-quality-bottom {
    margin-top: 25px;
    color: rgba(63, 54, 54, 0.6);
    font-size: 12px;
    line-height: 1.5;
    padding-left: calc(28.7% + 28px);
    display: flex;
    justify-content: flex-end;
    align-items: flex-start;
    gap: 16px;
  }

  .professional-quality .professional-quality-arraw {
    display: none;
    gap: 20px;
  }

  @media (max-width: 1600px) {
    .professional-quality .professional-quality-head .professional-quality-item::before {
      width: calc(100vw - 97px);
    }
  }

  @media (max-width: 1300px) {
    .professional-quality .professional-quality-head {
      font-size: 16px;
    }
    .professional-quality .professional-quality-fixed {
      font-size: 22px;
    }
    .professional-quality .professional-quality-other {
      font-size: 22px;
    }
    .professional-quality .professional-quality-fixed .professional-quality-item:last-child,
    .professional-quality .professional-quality-other .professional-quality-item:last-child {
      font-size: 26px;
    }
    .professional-quality .professional-quality-sm {
      font-size: 12px;
    }
  }

  @media (max-width: 991px) {
      .professional-quality {
    padding: 47px 0 47px 0;
  }
    .professional-quality .professional-quality-left {
      width: auto;
    }
    .professional-quality .professional-quality-head .professional-quality-item::before {
      width: calc(100vw - 65px);
    }
    .professional-quality .professional-quality-head {
      font-size: 14px;
      width: 97px;
      flex: none;
    }
    .professional-quality .icon {
      width: 69px;
    }
    .professional-quality .professional-quality-fixed {
      width: 120px;
      font-size: 18px;
      flex: none;
      border-radius: 10px;
    }

    .professional-quality .professional-quality-other {
      font-size: 18px;
    }
    .professional-quality .professional-quality-fixed .professional-quality-item:last-child,
    .professional-quality .professional-quality-other .professional-quality-item:last-child {
      font-size: 18px;
    }

    .professional-quality .professional-quality-item {
      padding: 20px 5px;
    }

    .professional-quality .professional-quality-title {
      font-size: 24px;
        text-align: left;
      margin-bottom:50px;
    }
    .professional-quality .professional-quality-sub {
      font-size: 14px;
    }
    .professional-quality .professional-quality-flex {
      margin-top: 20px;
      gap: 10px;
    }
    .professional-quality .professional-quality-tag {
      width: 40px;
      transform: translate(calc(50% + 8px), -50%);
    }
    .professional-quality .professional-quality-item svg {
      width: 25px;
    }
    .professional-quality .professional-quality-other {
      max-width: 100%;
      width: 120px;
      flex-shrink: 0;
      flex: none;
    }
    .professional-quality .professional-quality-right {
      overflow-x: auto;
      border-radius: 10px;
    }
    .professional-quality .professional-quality-right::-webkit-scrollbar {
      width: 6px;
      height: 6px;
    }

    .professional-quality .professional-quality-right::-webkit-scrollbar-track {
      background: transparent;
      border-radius: 6px;
    }

    .professional-quality .professional-quality-right::-webkit-scrollbar-thumb {
      background-color: #aaa;
      border-radius: 6px;
    }

    .professional-quality .professional-quality-right::-webkit-scrollbar-thumb:hover {
      background-color: #555;
    }
    .professional-quality .professional-quality-column {
      padding: 0;
      padding-bottom: 0px;
    }
    .professional-quality .professional-quality-bottom {
      padding-left: 0;
      margin-top: 20px;
    }
    .professional-quality .professional-quality-head .professional-quality-item {
    padding:20px 5px;
}
    .professional-quality .professional-quality-text {
    margin-top: 7px;
    font-size: 20px;
}
  }

  @media (max-width: 460px) {
    .professional-quality .professional-quality-right {
      overflow-x: hidden;
    }
    .professional-quality .professional-quality-other {
      width: 100%;
    }
    .professional-quality .professional-quality-head .professional-quality-item::before {
      width: calc(100vw - 48px);
    }
    .professional-quality .professional-quality-arraw {
      display: flex;
    }
    .professional-quality .professional-quality-arraw-item {
      cursor: pointer;
      opacity: 0.3;
      transition: opacity 0.3s ease;
    }
    .professional-quality .professional-quality-arraw-item:hover {
      opacity: 1;
    }
    .professional-quality .professional-quality-arraw svg {
      display: block;
    }
  }
</style>

